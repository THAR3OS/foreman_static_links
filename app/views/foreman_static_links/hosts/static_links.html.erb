<div id="breadcrumb">
  <div class="row form-group">
    <h1 class="col-md-8">
      <%= _('Static Links') %>
    </h1>
  </div>
</div>

<table class="<%= table_css_classes('table-condensed table-fixed') %>">
  <thead>
    <th><%= N_("Link") %></th>
  </thead>
  <% @linklist.each do |l| %>
    <tr>
      <td>
        <a href="<%= l[:url] %>" title="<%= l[:comment] %>" target="<%= l[:target] %>"><%= l[:title] %></a>
      </td>
    </tr>
  <% end %>
</table>

